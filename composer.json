{
	"name": "vicky-project/financial",
	"type": "laravel-module",
	"description": "Financial cashflow management to allocate monthly and yearly budget management. Build on top nwidart/laravel-modules",
	"authors": [
		{
			"name": "Vicky Rahman",
			"email": "pratamavr@gmail.com"
		}
	],
	"require": {
		"php": "^8.2",
		"laravel/framework": "^12.0",
		"nwidart/laravel-modules": "*",
		"icehouse-ventures/laravel-chartjs": "^4.2",
		"smalot/pdfparser": "^2.12",
		"maatwebsite/excel": "^3.1",
		"joshbrw/laravel-module-installer": "^1.0",
		"vicky-project/viewmanager-module": "dev-main",
		"vicky-project/menumanagement-module": "dev-main"
	},
	"extra": {
		"laravel": {
			"providers": [],
			"aliases": {}
		}
	},
	"autoload": {
		"psr-4": {
			"Modules\\Financial\\": "app/",
			"Modules\\Financial\\Database\\Factories\\": "database/factories/",
			"Modules\\Financial\\Database\\Seeders\\": "database/seeders/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Modules\\Financial\\Tests\\": "tests/"
		}
	}
}
